    "@fortawesome/react-fontawesome": "^0.1.4",
    "acorn": "^6.1.1",
    "axios": "*",
    "bootstrap": "^4.3.1",
    "env-cmd": "^8.0.2",
    "enzyme-adapter-react-16": "^1.12.1",
    "jest-enzyme": "^7.0.2",
    "jquery": "^3.4.0",
    "js-cookie": "^2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "npm-check-updates": "^3.1.8",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^0.2.2",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
