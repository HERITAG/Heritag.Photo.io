    "react-modal-image": "^2.3.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^2.1.8"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "HTTPS=false BROWSER='mozilla firefox' react-scripts start",
    "build:staging": "env-cmd .env.staging react-scripts build",
    "build:production": "env-cmd .env.production react-scripts build",
    "build:docker": "env-cmd .env.docker react-scripts build",
    "test": "react-scripts test",
    "test:once": "CI=true react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/serviceWorker.js",
      "!<rootDir>/src/index.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.9.0"
